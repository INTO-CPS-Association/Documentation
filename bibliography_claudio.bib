@book{Jantsch2003,
  title = {Modeling {{Embedded Systems}} and {{SoC}}'s},
  isbn = {978-1-55860-925-9},
  abstract = {This chapter starts with the observation that even two domains, that is, separate process networks, of the same models of computation (MoC) type may have a different time structure. For example, two S-MoC domains that are not connected can have different durations of their evaluation cycles. When connecting them, the relation between their respective time structures has to be defined. This relation can be constant and simple, or it can vary dynamically. The interface processes between the MoC domains define this relation. This chapter defines interface processes between different MoC types. It distinguishes between interfaces that add timing information and those that remove timing information. In general, both can be done in many different ways, but only one possible solution is described. Based on these interface processes, this chapter introduces an integrated MoC\textemdash{}consisting of different MoC domains and interface processes. Several profound issues emerge when connecting MoC domains, which go beyond the technical insertion and suppression of time information. This chapter proposes distinguishing between the time structures of different MoC domains and their relations to each other, the protocol used to model communication between different domains, and the modeling of delays on the channels between different domains.},
  url = {http://www.sciencedirect.com/science/article/pii/B9781558609259500081},
  author = {Jantsch, Axel},
  year = {2003},
  file = {D:\\Data\\Mendeley\\Jantsch\\Unknown\\Jantsch - 2004 - Modeling Embedded Systems and SoC's Concurrency and Time in Models of Computation.pdf},
