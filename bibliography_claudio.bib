journal = {IEEE Transactions on Computer-Aided Design of Integrated Circuits and Systems},
keywords = {Benchmark testing,Communication switching,Design optimization,Embedded systems,Energy consumption,Kernel,MPEG 4 Standard,Multiprocessing systems,Power system modeling,Reflection,Runtime,modeling,modelling,multiaccuracy model,multiaccuracy power modeling,performance transaction-level modeling,power estimation,power modeling and estimation,simulation},
