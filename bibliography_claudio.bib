isbn = {VO -},
keywords = {Access protocols,Computer architecture,Connectors,Data security,Delay,Electrical capacitance tomography,Software architecture,Software design,Software systems,Transaction databases,architectural mismatch,architecture description languages,behavior analysis,component behavior,components,software architecture,software design,systems analysis},
