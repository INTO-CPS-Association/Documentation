issn = {03062619},
journal = {Applied Energy},
keywords = {Conservation Voltage Reduction,Distribution network,Electric Vehicle,Energy conservation,Smart grid,Volt–VAR Optimization},
month = {may},
pages = {28--39},
title = {{Impact of EV penetration on Volt–VAR Optimization of distribution networks using real-time co-simulation monitoring platform}},
volume = {169},
year = {2016}
}
