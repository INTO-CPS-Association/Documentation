@article{Ujhelyi201580,
  title = {{{EMF}}-{{IncQuery}}: {{An}} Integrated Development Environment for Live Model Queries},
  volume = {98, Part 1},
  issn = {0167-6423},
  doi = {http://dx.doi.org/10.1016/j.scico.2014.01.004},
  abstract = {Abstract As model management platforms are gaining industrial attention, the importance of automated model querying techniques is also increasing. Several important engineering tasks supported by model-based tools \textendash{} such as well-formedness constraint validation or model transformations \textendash{} rely on efficiently evaluating model queries. If the models change rapidly or frequently, it is beneficial to provide live and incrementally evaluated queries that automatically propagate model changes to keep query results consistent. The current paper reports on the of EMF-IncQuery framework focusing on new features of its integrated development environment (such as query validation and visualization) and its support for integrating queries to existing applications (e.g. by auto-generated data bindings) built on top of the industry standard Eclipse Modeling Framework (EMF). Our approach is illustrated on a case study integrating well-formedness constraints to the Papyrus \{UML\} tool by live model queries of EMF-IncQuery with negligible additional manual programming effort.},
  number = {0},
  journal = {Science of Computer Programming},
  author = {Ujhelyi, Zolt\'an and Bergmann, G\'abor and Heged\"us, \'Abel and Horv\'ath, \'Akos and Izs\'o, Benedek and R\'ath, Istv\'an and Szatm\'ari, Zolt\'an and Varr\'o, D\'aniel},
  year = {2015},
  keywords = {{EMF},Integrated development environment,Live model query}
}

