@article{Busch2012,
abstract = {Standard coupling approaches for simulating coupled MBS/FEM systems may entail enormous CPU times in order to achieve a stable and accurate solution. To stabilize and speed up the solution process, semi-implicit coupling approaches can be applied successfully. In literature, semi-implicit coupling approaches have been introduced, where Jacobian information is exchanged between the subsystems using partial derivatives with respect to the state vectors of the subsystems. The drawback of these methods is that the numerical calculation of the Jacobians with respect to the whole state vector of the subsystems is very time-consuming. The key idea of the semi-implicit coupling technique presented here is to reduce the computational effort by using Jacobian information only with respect to the coupling variables.},
author = {Busch, Martin and Schweizer, Bernhard},
doi = {10.1007/s00419-011-0586-0},
file = {:C$\backslash$:/Users/clagms/Documents/Mendeley/Busch, Schweizer/Archive of Applied Mechanics/Busch, Schweizer - 2012 - Coupled simulation of multibody and finite element systems an efficient and robust semi-implicit coupling appr.pdf:pdf},
