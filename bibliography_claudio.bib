@article{Nunes2015,
abstract = {Borrelia miyamotoi, a relapsing fever spirochete, has been recently found in Ixodes ricinus ticks, however little is known about its spatial distribution and potential local impact on human health. Six hundred and forty ticks (447 nymphs and 193 adults) collected throughout Portugal were analyzed using two nested-PCR protocols, one targeting the fla flagellin[m1] gene, and the other the internal transcribed space region between the 5S and the 23S rRNA. As a result, Borrelia miyamotoi was detected, for the first time, in one I. ricinus nymph collected in the Lisboa district. In addition, a prevalence of 11{\%} (71/640) for B. burgdorferi sensu lato was obtained. Despite no human relapsing fever cases due to B. miyamotoi have been reported yet in Portugal, surveillance must be improved, to get a better insight into the prevalence and distribution of this spirochete in ticks.},
author = {Nunes, M{\'{o}}nica and Parreira, Ricardo and Lopes, N{\'{a}}dia and Maia, Carla and Carreira, Teresa and Sousa, Carmelita and Faria, Sofia and Campino, Lenea and Vieira, Maria L.},
