keywords = {Aerospace Engineering,Aircraft Vehicle Systems,FMI,Modelica,Rymd- och flygteknik,TLM},
language = {eng},
title = {{Early Insights on FMI-based Co-Simulation of Aircraft Vehicle Systems}},
url = {http://liu.diva-portal.org/smash/get/diva2:1153450/FULLTEXT01.pdf http://urn.kb.se/resolve?urn=urn:nbn:se:liu:diva-142400},
