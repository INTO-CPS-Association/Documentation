@article{Tomiyama2007,
abstract = {They argue that there are two kinds of complexity that arise in multi-disciplinary development of systems: complexity by design, which arises due to design decisions for the system; and intrinsic complexity of multi-disciplinarity, that arises to the physical relations between different components (e.g., dynamics and electromagnetics relate to each other with Force as a interface parameter). The last kind of complexity can be easily overlooked when developing components. They proceed to explain two kinds of coupling: constructive and destructive. Constructive is desired coupling. Destructive arises mostly due to intrinsic complexity and is usually unexpected/undesired. They illustrate this with a use case for a AGV material handling system. They show different problems that have shown up in this system due to destructive and constructive coupling, and how they affected certain functions of the system. They motivate the need for a tool that allows engineers to detect non-obvious interactions between physical parameters.},
author = {Tomiyama, T. and D'Amelio, V. and Urbanic, J. and ElMaraghy, W.},
doi = {10.1016/j.cirp.2007.05.044},
file = {:C$\backslash$:/Users/clagms/Documents/Mendeley/Tomiyama et al/CIRP Annals - Manufacturing Technology/Tomiyama et al. - 2007 - Complexity of Multi-Disciplinary Design.pdf:pdf},
issn = {00078506},
journal = {CIRP Annals - Manufacturing Technology},
keywords = {Complexity Management,Design,Knowledge},
