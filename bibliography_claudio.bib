  pages = {1781-1796},
  file = {D:\\Data\\Mendeley\\Heemels et al\\IEEE Transactions on Automatic Control\\Heemels et al. - 2010 - Networked Control Systems With Communication Constraints Tradeoffs Between Transmission Intervals, Delays and Pe.pdf}
